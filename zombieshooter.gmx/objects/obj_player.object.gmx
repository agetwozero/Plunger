    part_particles_create(global.ps,x + 64,y - 41,global.pt_smoke,20);
    instance_create(x + 64,y - 41 ,obj_bullet);
    
    if(weapon == 1){
        instance_create(x + 64 ,y - 41 ,obj_bullet);
        instance_create(x + 64 ,y - 41 ,obj_bullet);
    }
}
if(rot == 2){
    part_particles_create(global.ps,x + 32,y - 104,global.pt_smoke,20);
