            <kind>1</kind>
            <string>///Shoot 
if(aiming){
if(weapon_ready){
//Particles

part_system_depth(global.ps,depth - 5);
if(rot == 0){
    part_particles_create(global.ps,x - 29,y - 17,global.pt_smoke,20);
    instance_create(x - 29 ,y - 17 ,obj_bullet);
    
    if(weapon == 1){
