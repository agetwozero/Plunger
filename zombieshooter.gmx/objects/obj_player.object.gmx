        instance_create(x + 32 ,y - 104 ,obj_bullet);
    }
}
if(rot == 3){
    part_particles_create(global.ps,x - 61,y - 72,global.pt_smoke,20);
    instance_create(x - 61,y - 72 ,obj_bullet);
    
    if(weapon == 1){
        instance_create(x - 61 ,y - 72 ,obj_bullet);
        instance_create(x - 61 ,y - 72 ,obj_bullet);
    }
}

mf = true;
